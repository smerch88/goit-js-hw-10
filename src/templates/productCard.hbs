{{#each this}}
  <li class="card">
    <div class="img-wrap">
      <img src="{{flags.svg}}" alt="{{name.official}}" width="100" />
    </div>
    <div>
      <p>{{name.official}}</p>
      <p>Capital: {{capital}}</p>
      <p>Population: {{population}}</p>
      <p>Languages:
        {{#each languages}}{{#if @index}}, {{/if}}<span
          >{{this}}</span>{{/each}}</p>
    </div>
  </li>
{{/each}}